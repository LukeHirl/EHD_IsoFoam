FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.phase1 0
    volScalarFieldValue rhoE   0
);

regions
(
    sphereToCell   
    {
       centre (0 0 0);
       radius 0.0002;
       fieldValues
        (
            volScalarFieldValue alpha.phase1 1
        );
    }
);


FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];


internalField   uniform 0;

boundaryField
{
    upWall        
    {
        type            fixedValue;
		value			uniform 0;
    }

    downWall       
    {
        type            fixedValue;
		value			uniform 0;
    }

    leftandright      
    {
        type            fixedValue;
		value			uniform 0;
    }

    frontAndBack    
    {
        type            empty;
    }
}



alpha.water
{
    internalField   uniform 0;

    boundaryField
    {
        ".*"
        {
            type            zeroGradient;
        }
		"atmosphere"
		{
			type            inletOutlet;
			inletValue      uniform 0;
			value           uniform 0;
		}
		"inlet"
        {
			type            inletOutlet;
			inletValue      uniform 1;
			value           uniform 1;
		}
		"axis"
        {
            type            empty;
        }
		"maxY"
		{
			type            wedge;
		}
		"minY"
		{
			type            wedge;
		}
	}
}


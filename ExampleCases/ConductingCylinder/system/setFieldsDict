FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
    volScalarFieldValue rhoE_EM   0
);

regions
(
    boxToCell
    {
        box (-2 -2 -1) (2 2 1);

        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);

